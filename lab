[12:56 am, 22/11/2025] Dharani: import xml.etree.ElementTree as ET
import pandas as pd
import io as StringIO
print("xml module is workiing")
def parse_books (xml_text):
    root=ET.fromstring(xml_text)
    rows=[]

    for book in root.findall("book"):
        title=(book.findtext("title") or "").strip()
        author=(book.findtext("author") or "").strip()
        price=(book.findtext("price") or "").strip()

        try:
            price=float(price)
        except(TypeError,ValueError):
            price=None
        rows.append({"title":title , "author":author, "price":price})
    return pd.DataFrame(rows)

if name == "main":
    samples="""<books>
    <book><title>ABC</title><author>x</author><price>50</price>
    </book>
    </books>"""
    print(parse_books(samples))
