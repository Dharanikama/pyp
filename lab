[12:56 am, 22/11/2025] Dharani: import json

def is_complex_json(data):
    """Check if JSON contains nested dictionaries or lists."""
    for key, value in data.items():
        if isinstance(value, dict) or isinstance(value, list):
            return True
    return False


import json

def is_complex_json(data):
    """Check if JSON contains nested dictionaries or lists."""
    for key, value in data.items():
        if isinstance(value, dict) or isinstance(value, list):
            return True
    return False


def flatten_json(data, parent_key=""):
    flat = {}
    for key, value in data.items():
        new_key = f"{parent_key}.{key}" if parent_key else key

        # If value is a dictionary, flatten it recursively
        if isinstance(value, dict):
            flat.update(flatten_json(value, new_key))
        # If value is a list, flatten each item
        elif isinstance(value, list):
            for i, item in enumerate(value):
                flat.update(flatten_json({str(i): item}, new_key))
        else:
            flat[new_key] = value
    return flat


if __name__ == "__main__":
    json_str = '{"a":1,"b":{"c":2,"d":[3,{"e":4}]}}'
    data = json.loads(json_str)

    # Check for nested objects
    print("Contains Nested Complex Objects:", is_complex_json(data))

    # Flatten JSON
    result = flatten_json(data)
    print(result)
