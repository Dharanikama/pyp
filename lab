class student:
    def _init_ (self,name,year):
        self.name=name
        self.year=year
    def _str_(self):
        return f"stdent({self.name},{self.year})"
class GraduateStudent(student):
    def _init_(self,name,year,thesis):
        super()._init_(name,year)
        self.thesis=thesis
    def _str_(self):
        return f"student({self.name},{self.year},{self.thesis})"
g=GraduateStudent("neha",2025,"healthcare engineer")
with open("student.json","w") as f:
    json.dump({"name":g.name,"year":g.year,"thesis":g.thesis},f)
print(g)
